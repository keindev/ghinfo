{
  "name": "ghinfo",
  "version": "1.0.6",
  "description": "CLI util for generating a GitHub project information file",
  "homepage": "https://github.com/keindev/ghinfo#readme",
  "keywords": [
    "automated",
    "repo",
    "info",
    "cli",
    "github",
    "util"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "ghinfo": "bin/ghinfo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keindev/ghinfo.git"
  },
  "bugs": {
    "url": "https://github.com/keindev/ghinfo/issues"
  },
  "author": {
    "name": "Daniil Ryazanov",
    "email": "kein@tagproject.ru"
  },
  "license": "MIT",
  "scripts": {
    "prepare:clean": "rimraf lib coverage",
    "check:lint": "eslint src/**/*.ts",
    "check:spell": "cspell -c .vscode/cspell.json --no-summary \"README.md\" \"src/**/*.ts\"",
    "check:test": "jest",
    "test": "npm-run-all check:*",
    "build:ts": "tsc",
    "build:ghinfo": "node bin/ghinfo generate -d media -t utils",
    "build": "npm-run-all prepare:* check:* build:*",
    "util:changelog": "changelog generate",
    "util:commit-msg": "changelog lint --message HUSKY_GIT_PARAMS --lowercase-only",
    "util:pre-commit": "npm-run-all check:*"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "figures": "^3.2.0",
    "globby": "^11.0.2",
    "parse-github-url": "^1.0.2",
    "read-pkg": "^5.2.0",
    "stdout-update": "^1.6.5",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^15.0.0",
    "@types/parse-github-url": "^1.0.0",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "changelog-guru": "^1.0.0",
    "cspell": "^5.3.7",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
