{
    "name": "media-info",
    "version": "0.2.0",
    "description": "CLI utility for creating .mediainfo with full repository information",
    "homepage": "https://github.com/keindev/media-info#readme",
    "keywords": [
        "automated",
        "media",
        "info",
        "cli",
        "github"
    ],
    "main": "lib/MediaInfo.js",
    "module": "lib/MediaInfo.js",
    "types": "lib/MediaInfo.d.ts",
    "bin": {
        "mediainfo": "bin/mediainfo"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keindev/media-info.git"
    },
    "bugs": {
        "url": "https://github.com/keindev/media-info/issues"
    },
    "author": {
        "name": "Daniil Ryazanov",
        "email": "kein@tagproject.ru"
    },
    "license": "MIT",
    "scripts": {
        "prepare:clean": "rimraf lib coverage",
        "check:lint": "eslint src/**/*.ts --fix",
        "check:spell": "cspell -c .vscode/cspell.json --no-summary \"*.*\" \"**/*.ts\" \"**/*.js\"",
        "check:test": "jest",
        "build:ts": "tsc",
        "build:mediainfo": "node bin/mediainfo generate -d media -t utils",
        "build": "npm-run-all prepare:* check:* build:*",
        "util:changelog": "changelog generate",
        "util:commit-msg": "changelog lint --message HUSKY_GIT_PARAMS --lowercase-only",
        "util:pre-commit": "npm-run-all check:*"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "dependencies": {
        "@types/parse-github-url": "^1.0.0",
        "figures": "^3.1.0",
        "globby": "^10.0.1",
        "parse-github-url": "^1.0.2",
        "read-pkg": "^5.2.0",
        "stdout-update": "^1.3.7",
        "yargs": "^15.0.2"
    },
    "devDependencies": {
        "@types/jest": "^24.0.24",
        "@types/node": "^12.12.21",
        "@types/yargs": "^13.0.3",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "changelog-guru": "^1.0.0",
        "cspell": "^4.0.37",
        "eslint": "^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.1.1",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "ts-jest": "^24.2.0",
        "typescript": "3.7.3"
    }
}
